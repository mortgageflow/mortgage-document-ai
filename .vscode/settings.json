{
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "editor.colorDecorators": true,
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "tailwindCSS.validate": true,
    "tailwindCSS.experimental.classRegex": [
        ["clsx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
    ],
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "cSpell.words": [
        "clsx",
        "mortgageflow"
    ],
    "editor.formatOnSave": true,
    
}