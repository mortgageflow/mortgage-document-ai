{
    "name": "@mortgage-document-ai/models",
    "type": "commonjs",
    "version": "1.0.0",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "dist/*",
                "dist/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./utils": {
            "import": "./dist/utils.js",
            "require": "./dist/utils.js",
            "types": "./dist/utils.d.ts"
        },
        "./documents": {
            "import": "./dist/documents/index.js",
            "require": "./dist/documents/index.js",
            "types": "./dist/documents/index.d.ts"
        }
    },
    "scripts": {
        "dev": "tsc --watch",
        "clean": "rimraf dist",
        "build": "yarn clean && tsc -b"
    },
    "devDependencies": {
        "rimraf": "^5.0.5",
        "typescript": "5.2.2"
    },
    "dependencies": {
        "@google-cloud/documentai": "^8.1.0",
        "get-value": "^3.0.1",
        "rimraf": "^5.0.5"
    }
}